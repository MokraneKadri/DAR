package fr.upmc.dar.enums;

public enum SignInFields {

	 NAME ("/home"),
	 USERNAME ("username"),
	 EMAIL ("email"),
	 PASSWORD ("password"),
	 PASSWORDCONFIRM ("password"),
	 UNIVERSITY ("university"),
	 CURSUS ("cursus"),
	 SEXE("sexe");
	  private String name = "";
	   
	  //Constructeur
	  private SignInFields(String name) {
	
	
	    this.name = name;
	  }
	   
	  //renvoie la valeur du type enuméré
	  public String getRessourceUrl(){
	    return name.toString();
	  }
}
